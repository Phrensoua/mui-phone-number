{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "max-len": 0,
    "react/jsx-props-no-spreading": 0,
    "linebreak-style": 0
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalReturn": false
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
